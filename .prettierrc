{
  "bracketSpacing": true,
  "semi": true,
  "singleQuote": true,
  "printWidth": 1000,
  "tabWidth": 2,
  "complexity": [
    true,
    {
      "max": 20
    }
  ],
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "newline-per-chained-call": [
    true,
    {
      "ignoreChainWithDepth": 4
    }
  ],
  "no-shadowed-variable": false
}